  <template name="minit" value="var common = window['require']('qcloud.tdsql.common'),&#10;    component = common.component,&#10;    log = common.log;&#10;    &#10;var self = {&#10;    init: function(opts){&#10;        $content$&#10;    }&#10;};&#10;module.exports = self;" description="commonjs module init" toReformat="false" toShortenFQNames="true">
